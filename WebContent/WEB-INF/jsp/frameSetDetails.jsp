</div>
<!-- BEGIN CONTAINER -->
<div class="page-container">
	<!-- BEGIN SIDEBAR -->
	<jsp:include page="navigationSidebar.jsp" />
	<!-- END SIDEBAR -->
	<!-- BEGIN CONTENT -->
	<div class="page-content-wrapper">
		<div class="page-content">
			<!-- BEGIN STYLE CUSTOMIZER -->
			<!-- BEGIN PAGE HEADER-->
			<h3 class="page-title">
			显示框架管理 <small>框架集设置</small>
			</h3>
			<div class="page-bar">
				<ul class="page-breadcrumb">
					<li>
						<i class="fa fa-home"></i>
						<a href="<%=path%>/auth/dashboard.do">首页</a>
						<i class="fa fa-angle-right"></i>
					</li>
					<li>
						<a href="<%=path%>/frameset/list.do">框架集列表</a>
						<i class="fa fa-angle-right"></i>
					</li>
					<li>
						<a href="<%=path%>/frameset/details.do?frameSetId=${frameSetDetail.id}">框架集设置</a>
					</li>
				</ul>
			</div>
			<!-- END PAGE HEADER-->
			<!-- BEGIN Table ROWS -->
			<div class="row">
				<div class="col-md-12">
					<!-- BEGIN EXAMPLE TABLE PORTLET-->
					<div class="portlet box blue">
						<div class="portlet-title">
							<div class="caption">
								<i class="fa fa-edit"></i>${frameSetDetail.name}
							</div>
							<div class="tools">
								<a href="javascript:;" class="reload">
								</a>
							</div>
						</div>
						<div class="portlet-body">
							<div class="table-toolbar">
								<div class="row">
									<div class="col-md-6">
											<div class="actions">
												<a href="#new-confirm" data-toggle="modal" class="btn green" id="sample_editable_1_new">
													<i class="fa fa-plus"></i> 新建
												</a>
											</div>
									</div>
								</div>
							</div>
							<table class="table table-striped table-hover table-bordered" id="_editable_">
							<thead>
							<tr>
								<th>
									 名称
								</th>
								<th>
									 左
								</th>
								<th>
									 顶部
								</th>
								<th>
									 宽
								</th>
								<th>
									 高
								</th>
								<th>
									 隐藏
								</th>
								<th>
									 编辑
								</th>
								<th>
									 删除
								</th>
							</tr>
							</thead>
							<tbody>
							<c:forEach var="frame" items="${frameSetDetail.frames}">
							<tr>
								<td id="${frame.id}">
									 ${frame.name}
								</td>
								<td>
									 ${frame.left}
								</td>
								<td>
									 ${frame.top}
								</td>
								<td>
									 ${frame.width}
								</td>
								<td>
									 ${frame.height}
								</td>
								<td>
									 <c:choose>
											<c:when test="${frame.hidden == true}">
												<input type="checkbox" id="${frame.id}" checked>
											</c:when>
											<c:otherwise>
												<input type="checkbox" id="${frame.id}">
											</c:otherwise>
									</c:choose>
								</td>
								<td>
									<a class="edit" href="javascript:;">
									编辑 </a>
								</td>
								<td>
									<a class="delete" href="javascript:;">
									删除 </a>
								</td>
							</tr>
							</c:forEach>
							</tbody>
							</table>
						<!-- END EXAMPLE TABLE PORTLET-->
						</div>
					</div>
				</div>
			</div>
			<!-- END Table ROWS -->
			<!--  div class="clearfix" -->
			<!--  div-->
